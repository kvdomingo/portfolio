---
import CvSectionElement from "./CvSectionElement.astro";
interface Props {
  title: string;
  data: Array<Record<string, string>>;
}

const { title, data } = Astro.props;
---

<div class="mb-12">
  <h3 class="text-4xl">{title}</h3>

  <ul class="border-l border-gray-500">
    {data.map(d => <CvSectionElement data={d} />)}
  </ul>
</div>
