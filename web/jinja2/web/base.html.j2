        <a class="navbar-brand" href="{{ url('web:index') }}">
            {% if active_page == 'Home' %}
                <img src="https://res.cloudinary.com/kdphotography-assets/image/upload/v1/logo/logo-white.png" class="navbar-slick-brand" alt="KVD logo" />
            {% else %}
                <img src="https://res.cloudinary.com/kdphotography-assets/image/upload/v1/logo/logo-black.png" class="navbar-slick-brand" alt="KVD logo" />
            {% endif %}
        </a>
        <div class="overlay"></div>
        <button class="navbar-toggler ml-auto" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarNav">
            <ul class="navbar-nav ml-auto">
                <li class="nav-item">
                    <a class="nav-link" href="{{ url('web:index') }}">Home</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="{{ url('web:cv') }}">CV</a>
                </li>
                <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle" id="nav-portfolio" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">Portfolio</a>
                    <div class="dropdown-menu dropdown-menu-lg-right dropdown-default" aria-labelledby="nav-portfolio" style="background-color: rgba(255, 255, 255, 0.9);">
                        <a class="dropdown-item" href="{{ url('photography:index') }}">Photography</a>
                        <a class="dropdown-item" href="{{ url('svip:index') }}">Coursework</a>
                        <a class="dropdown-item" href="{{ url('dev:index') }}">Development</a>
                    </div>
                </li>
            </ul>
        </div>
        <div class="nav-blur"></div>
    </nav>

